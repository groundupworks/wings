  <modelVersion>4.0.0</modelVersion>
  <groupId>com.groundupworks.wings</groupId>
  <artifactId>wings-gcp</artifactId>
  <version>1.0.4</version>
  <packaging>aar</packaging>
  <name>Wings Google Cloud Print Module</name>
  <description>Android library for silently sharing photos to multiple endpoints</description>
  <url>http://www.groundupworks.com/wings/</url>
  <licenses>
    <license>
      <name>The Apache Software License, Version 2.0</name>
      <url>http://www.apache.org/licenses/LICENSE-2.0.txt</url>
      <distribution>repo</distribution>
    </license>
  </licenses>
  <developers>
    <developer>
      <id>groundupworks</id>
      <name>Ground Up Works</name>
    </developer>
  </developers>
  <scm>
    <connection>scm:git@github.com:groundupworks/wings.git</connection>
    <developerConnection>scm:git@github.com:groundupworks/wings.git</developerConnection>
    <url>https://github.com/groundupworks/wings</url>
  </scm>
  <dependencies>
    <dependency>
      <groupId>com.groundupworks.wings</groupId>
      <artifactId>wings</artifactId>
      <version>1.0.4</version>
      <scope>compile</scope>
    </dependency>
    <dependency>
      <groupId>com.groundupworks.android</groupId>
      <artifactId>google-auth</artifactId>
      <version>1.0.3</version>
      <scope>compile</scope>
      <exclusions>
        <exclusion>
          <artifactId>support-v4</artifactId>
          <groupId>com.android.support</groupId>
        </exclusion>
        <exclusion>
          <artifactId>rxandroid</artifactId>
          <groupId>io.reactivex</groupId>
        </exclusion>
      </exclusions>
    </dependency>
    <dependency>
      <groupId>io.reactivex</groupId>
      <artifactId>rxandroid</artifactId>
      <version>0.23.0</version>
      <scope>compile</scope>
    </dependency>
    <dependency>
